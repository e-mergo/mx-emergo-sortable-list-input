{
    "name": "sortablelistinput",
    "widgetName": "SortableListInput",
    "version": "1.0.0",
    "description": "Mendix custom widget for using an input field that enables sorting values.",
    "author": "E-mergo, Laurens Offereins",
    "engines": {
        "node": ">=16"
    },
    "license": "MIT",
    "config": {
        "projectPath": "../../<ProjectFolder>",
        "mendixHost": "http://localhost:8080",
